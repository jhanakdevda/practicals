#include <stdio.h>
int main()
{
    int n, i, j, min_idx;
    printf("Enter number of students: ");
    if (scanf("%d", &n) != 1 || n <= 0) return 0;
    float arr[n];
    for (i = 0; i < n; i++) scanf("%f", &arr[i]);
    for (i = 0; i < n - 1; i++)
    {
        min_idx = i;
        for (j = i + 1; j < n; j++)
            if (arr[j] < arr[min_idx]) min_idx = j;
        if (min_idx != i)
        {
            float temp = arr[i];
            arr[i] = arr[min_idx];
            arr[min_idx] = temp;
        }
    }
    int count = n < 5 ? n : 5;
    printf("Top %d scores (highest to lowest):\n", count);
    for (i = 0; i < count; i++) printf("%.2f\n", arr[n - 1 - i]);
    return 0;
}
